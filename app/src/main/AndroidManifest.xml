        android:theme="@style/Theme.DoAnAndroid"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ThongTinMainActivity"></activity>
        <activity android:name=".DangKyTaiKhoanManactivity"></activity>
        <activity android:name=".DatPhongKhachSanActivity"></activity>
    </application>

</manifest>